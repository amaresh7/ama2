pipeline{
 agent any
   stages{
     stage('build'){
       steps{
         git 'https://github.com/amaresh7/ama2.git'
       }
     }
     stage('test'){
       steps{
          sh label: '', script: 'mvn package'
          
        }
      }
     stage('deploy'){
       steps{
          echo "This is a deploy"
     }
   }
 }
}
