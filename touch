node('MAVEN') {
    stage('SCM') {
       git 'https://github.com/amaresh7/ama2.git'
}
    stage('mvn') {
       sh label: '', script: 'mvn package'
}
    stage('PBA') {
       archiveArtifacts 'target/*.jar'
}
    stage('email') {
       mail bcc: '', body: 'thank you', cc: '', from: '', replyTo: '', subject: 'Jenkins job', to: 'adash6961@gmail.com'
}
}
